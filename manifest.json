{
    "manifest_version": 2,
    "name": "URL Expander",
    "version": "1.0",
    "description": "Expand shortened URLs",
    "icons": {
      "16": "icon.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "permissions": ["tabs"],
    "browser_action": {
      "default_popup": "popup.html"
    }
  }
  